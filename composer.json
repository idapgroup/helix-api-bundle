{
    "name": "idapgroup/helix-api-bundle",
    "description": "Symphony wrapper over Helix api",
    "type": "symfony-bundle",
    "license": "MIT",
    "version": "0.1.0",
    "require": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "guzzlehttp/guzzle": "^7.9",
        "symfony/console": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.3.*",
        "symfony/yaml": "6.3.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IdapGroup\\HelixApiBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "idap"
        }
    ]
}
